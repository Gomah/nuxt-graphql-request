{
  "name": "nuxt-graphql-request",
  "version": "4.0.0",
  "description": "Easy GraphQL client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxtjs",
    "nuxt.js",
    "graphql",
    "graphql-request",
    "nuxt-graphql",
    "nuxt-graphql-request"
  ],
  "typings": "types/index.d.ts",
  "main": "lib/module.js",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "repository": "https://github.com/Gomah/nuxt-graphql-request",
  "homepage": "https://github.com/Gomah/nuxt-graphql-request",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test:system": "NODE_ENV=test jest --testRegex \"/test/system/(.+)\\.test\\.ts$\"",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn test:lint && NODE_ENV=test jest",
    "test:lint": "eslint --ext .js, lib/module.js"
  },
  "peerDependencies": {
    "graphql": "14.x || 15.x"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "graphql-request": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.15.1",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.31",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "nuxt": "^2.15.1",
    "prettier": "^2.2.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "standard-version": "^9.1.1"
  }
}
