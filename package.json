{
  "name": "nuxt-graphql-request",
  "version": "8.0.1",
  "description": "Easy GraphQL client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxtjs",
    "nuxt.js",
    "graphql",
    "graphql-request",
    "nuxt-graphql",
    "nuxt-graphql-request"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs"
    }
  },
  "build": {
    "entries": ["./src/utils"]
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": ["dist"],
  "repository": "https://github.com/Gomah/nuxt-graphql-request",
  "homepage": "https://github.com/Gomah/nuxt-graphql-request",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && standard-version && git push --follow-tags && npm publish",
    "lint": "biome check .",
    "prepack": "nuxt-module-build build",
    "prepare": "husky install",
    "prepublishOnly": "yarn test",
    "test": "yarn dev:prepare && vitest run",
    "test:lint": "biome check .",
    "test:watch": "yarn dev:prepare && vitest watch"
  },
  "peerDependencies": {
    "graphql": "14.x || 15.x || 16.x"
  },
  "dependencies": {
    "@nuxt/kit": "3.11.2",
    "@rollup/plugin-graphql": "2.0.4",
    "graphql-request": "7.0.1",
    "remeda": "1.61.0",
    "unplugin": "1.10.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@biomejs/biome": "1.7.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "@nuxt/devtools": "1.2.0",
    "@nuxt/module-builder": "0.6.0",
    "@nuxt/schema": "3.11.2",
    "@nuxt/test-utils": "3.12.1",
    "@nuxt/types": "2.17.3",
    "@pinia/nuxt": "0.5.1",
    "@types/node": "20.12.11",
    "cheerio": "1.0.0-rc.12",
    "graphql": "16.8.1",
    "graphql-tag": "2.12.6",
    "husky": "9.0.11",
    "jest-serializer-html": "7.1.0",
    "nuxt": "3.11.2",
    "pinia": "2.1.7",
    "standard-version": "9.5.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
