{
  "name": "nuxt-graphql-request",
  "version": "7.0.4",
  "description": "Easy GraphQL client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxtjs",
    "nuxt.js",
    "graphql",
    "graphql-request",
    "nuxt-graphql",
    "nuxt-graphql-request"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs"
    }
  },
  "build": {
    "entries": [
      "./src/utils"
    ]
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/Gomah/nuxt-graphql-request",
  "homepage": "https://github.com/Gomah/nuxt-graphql-request",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && standard-version && git push --follow-tags && npm publish",
    "lint": "eslint .",
    "prepack": "nuxt-module-build build",
    "prepare": "husky install",
    "prepublishOnly": "yarn test",
    "test": "yarn dev:prepare && vitest run",
    "test:lint": "eslint --ext .ts, src/",
    "test:watch": "yarn dev:prepare && vitest watch"
  },
  "peerDependencies": {
    "graphql": "14.x || 15.x || 16.x"
  },
  "dependencies": {
    "@buttercup/fetch": "0.2.1",
    "@nuxt/kit": "3.8.1",
    "@rollup/plugin-graphql": "2.0.4",
    "graphql-request": "6.1.0",
    "remeda": "1.29.0",
    "unplugin": "1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@nuxt/devtools": "^1.0.0",
    "@nuxt/module-builder": "0.5.4",
    "@nuxt/schema": "3.8.1",
    "@nuxt/test-utils": "3.8.1",
    "@nuxt/types": "2.17.2",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@pinia/nuxt": "0.5.1",
    "@types/node": "20.9.0",
    "@types/webpack": "5.28.5",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vue": "9.18.1",
    "graphql": "16.8.1",
    "graphql-tag": "2.12.6",
    "husky": "8.0.3",
    "jest-serializer-html": "7.1.0",
    "nuxt": "3.8.1",
    "pinia": "2.1.7",
    "prettier": "3.0.3",
    "standard-version": "9.5.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  }
}
